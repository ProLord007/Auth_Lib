---
import BaseContentTable from "@components/content-table/BaseContentTable.svelte";
import { ConfigGlob, getSections } from "src/lib/content.js";

interface Props {
	title: string;
}

const sections = await getSections(
	import.meta.glob("/content/oauth/*/config.json") as ConfigGlob,
	await Astro.glob("/content/oauth/*/*.md")
);
---

<BaseContentTable
	contents={[
		{
			title: "",
			id: "",
			sections
		}
	]}
	title="OAuth integration"
	currentPath={Astro.url.pathname}
/>
