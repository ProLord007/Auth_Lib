{
    "name": "lucia-sveltekit",
    "version": "0.8.8",
    "description": "A simple authentication library for SvelteKit",
    "main": "index.js",
    "types": "index.d.ts",
    "module": "index.js",
    "svelte": "index.js",
    "type": "module",
    "files": [
        "**/*"
    ],
    "scripts": {
        "build": "svelte-package"
    },
    "keywords": [
        "sveltekit",
        "lucia",
        "authentication"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": "./index.js",
        "./client": "./client.js",
        "./load": "./load/index.js"
    },
    "typesVersions": {
        "*": {
            "client": [
                "client.d.ts"
            ],
            "types": [
                "types.d.ts"
            ],
            "load": [
                "load/index.d.ts"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pilcrowOnPaper/lucia-sveltekit",
        "directory": "packages/lucia-sveltekit"
    },
    "author": "pilcrowonpaper",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-typescript": "^8.2.1",
        "@sveltejs/package": "1.0.0-next.3",
        "@types/bcryptjs": "^2.4.2",
        "@types/cli-color": "^2.0.2",
        "@types/cookie": "^0.5.1",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/node": "^18.6.2",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "build-esm": "^4.2.2",
        "esbuild": "^0.12.1",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.3.0",
        "rollup": "^2.48.0",
        "rollup-plugin-esbuild": "^4.2.3",
        "rollup-plugin-multi-input": "^1.3.0",
        "svelte": "^3.38.2",
        "svelte-preprocess": "^4.10.7",
        "svelte2tsx": "^0.5.13",
        "typescript": "^4.7.4",
        "vite": "^3.0.0"
    },
    "dependencies": {
        "cli-color": "^2.0.3",
        "cookie": "^0.5.0",
        "jsonwebtoken": "^8.5.1",
        "nanoid": "^4.0.0",
        "node-fetch": "^3.2.10"
    }
}