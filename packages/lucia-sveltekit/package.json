{
    "name": "lucia-sveltekit",
    "version": "0.13.2",
    "description": "A simple authentication library for SvelteKit",
    "main": "index.js",
    "types": "index.d.ts",
    "module": "index.js",
    "svelte": "index.js",
    "type": "module",
    "files": [
        "**/*"
    ],
    "scripts": {
        "build": "tsc && cp ./package.json ./dist && cp ./README.md ./dist && cp .npmignore dist"
    },
    "keywords": [
        "sveltekit",
        "lucia",
        "authentication"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": "./index.js",
        "./client": "./client.js",
        "./load": "./load/index.js",
        "./adapter": "./adapter/index.js"
    },
    "typesVersions": {
        "*": {
            "client": [
                "client.d.ts"
            ],
            "types": [
                "types.d.ts"
            ],
            "load": [
                "load/index.d.ts"
            ],
            "adapter": [
                "adapter/index.d.ts"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pilcrowOnPaper/lucia-sveltekit",
        "directory": "packages/lucia-sveltekit"
    },
    "author": "pilcrowonpaper",
    "license": "MIT",
    "devDependencies": {
        "@types/cli-color": "^2.0.2",
        "@types/cookie": "^0.5.1",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/node": "^18.6.2",
        "prettier": "^2.3.0",
        "svelte": "^3.50.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "cli-color": "^2.0.3",
        "cookie": "^0.5.0",
        "nanoid": "^4.0.0"
    }
}
