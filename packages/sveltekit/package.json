{
	"name": "@lucia-auth/sveltekit",
	"version": "0.1.5",
	"description": "Lucia integration for SvelteKit",
	"main": "index.js",
	"types": "index.d.ts",
	"module": "index.js",
	"type": "module",
	"files": [
		"**/*"
	],
	"scripts": {
		"build": "rm -rf ./dist/* && tsc && cp ./package.json ./dist && cp ./README.md ./dist && cp .npmignore dist",
		"package": "pnpm build && cd dist && pnpm install --no-frozen-lockfile && pnpm publish --no-git-checks --access public && cd ../"
	},
	"keywords": [
		"lucia",
		"sveltekit",
		"lucia-auth",
		"authentication",
		"svelte",
		"auth"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/lucia-auth",
		"directory": "packages/lucia"
	},
	"author": "pilcrowonpaper",
	"license": "MIT",
	"exports": {
		"./package.json": "./package.json",
		".": "./index.js",
		"./load": "./load/index.js",
		"./client": "./client/index.js"
	},
	"typesVersions": {
		"*": {
			"client": [
				"client/index.d.ts"
			],
			"load": [
				"load/index.d.ts"
			]
		}
	},
	"devDependencies": {
		"@types/node": "^18.6.2",
		"lucia-auth": "workspace:*",
		"svelte": "^3.52.0"
	}
}
